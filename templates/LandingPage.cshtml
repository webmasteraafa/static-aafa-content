@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.LandingPage>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = "landingmaster.cshtml";
}

@{
@Umbraco.RenderMacro("RAZORWidgetDropper", new {Node="4564",})
	if (CurrentPage.HasValue("sidebar"))
                {
					
                    var sidebarStatusText = Umbraco.GetPreValueAsString(CurrentPage.sidebar);
					
                    
					if (@sidebarStatusText == "right") 
                    {
                        Html.RenderPartial("rightsidebar");
                    }
					else if (@sidebarStatusText == "left")
                    {
                        Html.RenderPartial("leftsidebar");
                    }
                    else if (@sidebarStatusText == "full")
                    {
                        Html.RenderPartial("fullcontent");
					}
                    
                }
    }